  "version": "1.0.1",
  "description": "A monorepo for various dApps",
  "license": "UNLICENSED",
  "workspaces": [
    "frontend",
    "injected-connector",
    "dapps-lib"
  ],
  "scripts": {
    "docker": "docker-compose   -f infra/docker-compose.base.yml   -f infra/docker-compose.build.yml   -f infra/docker-compose.dev.yml   --project-name comtech-improvement   --project-directory ./",
